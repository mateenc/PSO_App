<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".HomeScreen"
    android:id="@+id/hello_world">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ON"
        android:id="@+id/Outlet1On"
        android:onClick="Outlet1OnButtonClick"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/Outlet1Off" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OFF"
        android:id="@+id/Outlet1Off"
        android:onClick="Outlet1OffButtonClick"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Outlet 2"
        android:id="@+id/textView2"
        android:layout_alignBottom="@+id/Outlet2On"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/Outlet1Power"
        android:layout_alignBottom="@+id/Outlet1On"
        android:layout_toLeftOf="@+id/Outlet1On"
        android:layout_alignTop="@+id/textView2"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Outlet 1"
        android:id="@+id/textView4"
        android:layout_alignBottom="@+id/Outlet1On"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OFF"
        android:id="@+id/Outlet2Off"
        android:onClick="Outlet2OffButtonClick"
        android:layout_below="@+id/Outlet1Off"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="54dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ON"
        android:id="@+id/Outlet2On"
        android:onClick="Outlet2OnButtonClick"
        android:layout_alignTop="@+id/Outlet2Off"
        android:layout_toLeftOf="@+id/Outlet2Off" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/Outlet1RealPowerLabel"
        android:layout_below="@+id/textView4"
        android:textSize = "16sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/Outlet1ApparentPowerLabel"
        android:textSize = "16sp"
        android:layout_below="@+id/Outlet1RealPowerLabel"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/Outlet2RealPowerLabel"
        android:layout_below="@+id/textView2"
        android:layout_alignLeft="@+id/Outlet1RealPowerLabel"
        android:layout_alignStart="@+id/Outlet1RealPowerLabel"
        android:textSize = "16sp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/Outlet2ApparentPowerLabel"
        android:textSize = "16sp"
        android:layout_below="@+id/Outlet2RealPowerLabel"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get Power"
        android:id="@+id/getPowerButton"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/Outlet2Off"
        android:layout_alignEnd="@+id/Outlet2Off"
        android:onClick="getPowerOnClick" /><![CDATA[















    Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/button"
        android:layout_below="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="97dp" />








]]>
</RelativeLayout>
